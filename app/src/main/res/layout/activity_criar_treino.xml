<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_tablet">

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="130dp"
        android:layout_height="125dp"
        app:srcCompat="@drawable/img"
        tools:layout_editor_absoluteX="3dp"
        tools:layout_editor_absoluteY="3dp" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="132dp"
        android:layout_height="38dp"
        android:layout_marginHorizontal="300dp"
        android:layout_marginVertical="410dp"
        android:text="Equipa" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="132dp"
        android:layout_height="38dp"
        android:layout_marginLeft="300dp"
        android:layout_marginTop="340dp"
        android:text="Hora" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="132dp"
        android:layout_height="38dp"
        android:layout_marginHorizontal="300dp"
        android:layout_marginVertical="200dp"
        android:text="Local" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="132dp"
        android:layout_height="38dp"
        android:layout_marginHorizontal="300dp"
        android:layout_marginVertical="270dp"
        android:text="Data" />

    <EditText
        android:id="@+id/local"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="600dp"
        android:layout_marginVertical="200dp"
        android:background="@drawable/bg_edittext"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="local" />

    <EditText
        android:id="@+id/data"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="600dp"
        android:layout_marginVertical="270dp"
        android:background="@drawable/bg_edittext"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="data" />

    <EditText
        android:id="@+id/hora"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="600dp"
        android:layout_marginVertical="340dp"
        android:background="@drawable/bg_edittext"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="hora" />

    <Button
        android:id="@+id/criar22"
        android:layout_width="130dp"
        android:layout_height="76dp"
        android:layout_marginHorizontal="500dp"
        android:layout_marginVertical="600dp"
        android:background="@drawable/button"
        android:text="Guardar"
        android:textColor="@color/black"
        app:backgroundTint="@null" />

    <ListView
        android:id="@+id/window_list2"
        android:layout_width="300dp"
        android:layout_height="100dp"
        android:layout_marginHorizontal="570dp"
        android:layout_marginVertical="410dp"
        android:background="@drawable/bg_edittext"
        android:choiceMode="multipleChoice"
        android:ems="10"
        tools:ignore="MissingConstraints">

    </ListView>

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="73dp"
        android:layout_marginHorizontal="150dp"
        android:layout_marginVertical="30dp"
        android:text="Criar Treino"
        android:textSize="48sp" />
</RelativeLayout>