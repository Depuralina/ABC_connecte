<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CriarAtleta"
    android:background="@drawable/background_tablet">


    <TextView
        android:id="@+id/textView24"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="100dp"
        android:layout_marginVertical="260dp"
        android:text="  Criar \nAdmin"
        android:textColor="@color/black"
        android:textSize="30dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/signUpErrorView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.957"
        tools:layout_editor_absoluteX="0dp" />

    <EditText
        android:id="@+id/nome"
        android:layout_width="10000dp"
        android:layout_height="60dp"
        android:layout_marginHorizontal="300dp"
        android:autofillHints=""
        android:background="@drawable/bg_edittext"
        android:ems="10"
        android:hint="Nome"
        android:inputType="textEmailAddress"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.044"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.2"
        android:layout_marginTop="650dp"
        tools:targetApi="o" />

    <EditText
        android:id="@+id/numero"
        android:layout_width="10000dp"
        android:layout_height="60dp"
        android:layout_marginHorizontal="300dp"
        android:autofillHints=""
        android:background="@drawable/bg_edittext"
        android:ems="10"
        android:hint="Numero"
        android:inputType="textEmailAddress"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.044"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.2"
        android:layout_marginTop="550dp"
        tools:targetApi="o" />

    <EditText
        android:id="@+id/idade"
        android:layout_width="10000dp"
        android:layout_height="60dp"
        android:layout_marginHorizontal="300dp"
        android:autofillHints=""
        android:background="@drawable/bg_edittext"
        android:ems="10"
        android:hint="Idade"
        android:inputType="textEmailAddress"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.044"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.2"
        android:layout_marginTop="450dp"
        tools:targetApi="o" />

    <EditText
        android:id="@+id/genero"
        android:layout_width="10000dp"
        android:layout_height="60dp"
        android:layout_marginHorizontal="300dp"
        android:autofillHints=""
        android:background="@drawable/bg_edittext"
        android:ems="10"
        android:hint="Genero"
        android:inputType="textEmailAddress"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.044"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.2"
        android:layout_marginTop="350dp"
        tools:targetApi="o"  />

    <EditText
        android:id="@+id/pass"
        android:layout_width="10000dp"
        android:layout_height="60dp"
        android:layout_marginHorizontal="300dp"
        android:autofillHints=""
        android:background="@drawable/bg_edittext"
        android:ems="10"
        android:hint="Password"
        android:inputType="textPassword"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.044"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.2"
        android:layout_marginTop="250dp"
        tools:targetApi="o" />

    <EditText
        android:id="@+id/mail"
        android:layout_width="10000dp"
        android:layout_height="60dp"
        android:layout_marginHorizontal="300dp"
        android:layout_marginTop="150dp"
        android:autofillHints=""
        android:background="@drawable/bg_edittext"
        android:ems="10"
        android:hint="Email"
        android:inputType="textEmailAddress"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.044"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.2"
        tools:targetApi="o" />

    <EditText
        android:id="@+id/user"
        android:layout_width="10000dp"
        android:layout_height="60dp"
        android:layout_marginHorizontal="300dp"
        android:autofillHints=""
        android:background="@drawable/bg_edittext"
        android:ems="10"
        android:hint="Username"
        android:inputType="textEmailAddress"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.044"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.2"
        android:layout_marginTop="50dp"
        tools:targetApi="o" />

    <Button
        android:id="@+id/criar"
        android:layout_width="167dp"
        android:layout_height="72dp"
        android:layout_marginHorizontal="1050dp"
        android:background="@drawable/button"
        android:text="criar"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:backgroundTint="@null"
        android:layout_marginTop="645dp" />


    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="300dp"
        android:layout_height="250dp"
        android:layout_marginHorizontal="1000dp"
        android:layout_marginVertical="15dp"
        app:srcCompat="@drawable/img" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="288dp"
        android:layout_height="432dp"
        android:layout_marginVertical="100dp"
        app:srcCompat="@drawable/empresario" />

</RelativeLayout>